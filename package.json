{
  "name": "typespeed-swagger",
  "version": "1.1.4",
  "description": "The Swagger plugin for the TypeSpeed framework.",
  "main": "dist/index.js",
  "scripts": {
    "watch": "nodemon ./ ",
    "start": "ts-node -C ttypescript app/main.ts",
    "build": "ttsc index.ts",
    "postbuild": "cp index.d.ts dist/index.d.ts",
    "test": "mocha --require ts-node/register test/**/*.ts --exit",
    "test-with-coverage": "nyc --reporter=lcov npm test"
  },
  "homepage": "https://github.com/speedphp/typespeed-swagger#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speedphp/typespeed-swagger.git"
  },
  "author": "speedphp",
  "license": "MIT",
  "dependencies": {
    "reflect-metadata": "0.1.13",
    "swagger-ui-express": "4.6.3",
    "typescript-rtti": "latest",
    "typespeed": "latest"
  },
  "devDependencies": {
    "@types/node": "18.0.6",
    "@types/swagger-ui-express": "4.1.3",
    "@types/mocha": "^10.0.1",
    "chai": "4.3.7",
    "chai-http": "^4.4.0",
    "mocha": "10.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "ttypescript": "1.x",
    "typescript": "4.7.4"
  },
  "keywords": [
    "typespeed",
    "express",
    "typescript-rtti",
    "jsdoc",
    "swagger",
    "api",
    "rtti",
    "middleware",
    "reflection",
    "reflect"
  ]
}
